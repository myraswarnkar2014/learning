penColour(colour_random());
for (var count2 = 0; count2 < 8; count2++) {
  for (var count = 0; count < 4; count++) {
      moveForward(50);
    turnLeft(90);
  }
  jumpForward(50);
  turnRight(45);
}
for (var count4 = 0; count4 < 8; count4++) {
  for (var count3 = 0; count3 < 3; count3++) {
      turnLeft(60);
    moveForward(50);
    turnLeft(180);
  }
  jumpForward(50);
  turnRight(45);
}
